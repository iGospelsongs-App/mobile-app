{
  "expo": {
    "name": "igospelsongs",
    "slug": "igospelsongs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Igospel to access your photos.",
          "savePhotosPermission": "Allow igospel to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.omatsola619.igospelsongs"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bc28201a-39eb-4cc2-aa38-6eacf3cb4a5f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bc28201a-39eb-4cc2-aa38-6eacf3cb4a5f"
    }
  }
}
